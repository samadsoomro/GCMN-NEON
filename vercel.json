{
    "functions": {
        "server/index.ts": {
            "runtime": "@vercel/node"
        }
    },
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "server/index.ts"
        },
        {
            "source": "/(.*)",
            "destination": "server/index.ts"
        }
    ]
}