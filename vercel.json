{
    "version": 2,
    "builds": [
        {
            "src": "server/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server/index.ts"
        },
        {
            "src": "/server/uploads/(.*)",
            "dest": "server/index.ts"
        },
        {
            "src": "/(.*)",
            "dest": "server/index.ts"
        }
    ]
}